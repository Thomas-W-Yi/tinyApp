<%- include('../partials/head.ejs') %>

<main>
    <div class="jumbotron">
      <h2>Variable</h2>
      <p><%= tagline %></p>

      <ul>
        <% mascots.forEach((mascot) => { %>
          <li>
              <strong><%= mascot.name %></strong>
              representing <%= mascot.organization %>, born <%= mascot.birth_year   %>
          </li>
        <% }); %>
      </ul>
    </div>



  
</main>

